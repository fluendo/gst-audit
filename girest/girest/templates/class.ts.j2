{# Class template #}
{% if is_enum %}
export namespace {{ name }} {
{% for method in methods %}
{{ method }}
{% endfor %}
}
{% else %}
export class {{ name }}{% if base_url and extends_gobject %} extends GObjectBase{% endif %} {
{% if not base_url or not extends_gobject %}
{% for prop in properties %}
  {{ prop.name }}{{ prop.assertion }}: {{ prop.type }};
{% endfor %}

{% endif %}
{% for constructor in constructors %}
{{ constructor }}
{% endfor %}
{% if constructors and methods %}

{% endif %}
{% for method in methods %}
{{ method }}
{% endfor %}
}
{% endif %}