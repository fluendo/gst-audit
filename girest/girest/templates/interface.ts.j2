{# Generic interface template - fallback for interfaces without specific templates #}
{% if interface.has_parent %}
{% if interface.properties %}
export interface {{ interface.name }} extends {{ interface.parent }} {
{% for prop in interface.properties %}
  {{ prop.name }}{{ prop.optional }}: {{ prop.type }};
{% endfor %}
}
{% else %}
export interface {{ interface.name }} extends {{ interface.parent }} {}
{% endif %}
{% else %}
{% if interface.properties %}
export interface {{ interface.name }} {
{% for prop in interface.properties %}
  {{ prop.name }}{{ prop.optional }}: {{ prop.type }};
{% endfor %}
}
{% else %}
export interface {{ interface.name }} {}
{% endif %}
{% endif %}
