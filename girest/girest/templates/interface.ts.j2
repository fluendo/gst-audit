{# Generic interface template - fallback for interfaces without specific templates #}
{% if has_parent %}
{% if properties %}
export interface {{ name }} extends {{ parent }} {
{% for prop in properties %}
  {{ prop.name }}{{ prop.optional }}: {{ prop.type }};
{% endfor %}
}
{% else %}
export interface {{ name }} extends {{ parent }} {}
{% endif %}
{% else %}
{% if properties %}
export interface {{ name }} {
{% for prop in properties %}
  {{ prop.name }}{{ prop.optional }}: {{ prop.type }};
{% endfor %}
}
{% else %}
export interface {{ name }} {}
{% endif %}
{% endif %}
